<div class="navbar-default sidebar" role="navigation">
    <div class="sidebar-nav navbar-collapse"></div>
    </div>
</nav>
<br/>

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Request Details</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <p><a href="/<%=DEFAULT_URL%>/<%=params[:id]%>/requests"> Show Requests </a></p>
                </div>                       
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th>Key</th>
                                    <th>Value</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%keys=@request_d.attributes.keys%>
                                <%keys.each do |key|%>
                                    <tr class="even gradeC">
                                        <td><%=key%></td>
                                        <td><%=@request_d[key]%></td> 
                                    </tr>
                                <% end %>    
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
     
  