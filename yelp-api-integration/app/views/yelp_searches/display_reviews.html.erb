<div class="col-md-10 col-md-offset-1">
	<div class="page-header">
    <h1>Reviews</h1>
    <p class="pull-right" style="margin-top:-42px"> 
    <a href="/"><button type="submit" class="btn btn-primary">Back to Search</button> </a>
	</div>	
	<table class="table table-striped">
    <thead>
      <tr>
        <th>Yelp Id</th>
        <th>Review Text</th>
        <th>Created Time</th>
        <th>User ID</th>
        <th>User Name</th>
        <th>User Image</th>
      </tr>
    </thead>
    <tbody>
      <%@reviews.each do |review| %>
        <tr>
          <td><%=review.yelp_id%></td>
          <td><%=review.body%></td>
          <td><%=review.time_created%></td>
          <td><%=review.user_id%></td>
          <td><%=review.user_name%></td>
          <td><img src=<%=review.user_photo_url%>></img></td>
        </tr>
      <%end%>  
    </tbody>
  </table>
</div>